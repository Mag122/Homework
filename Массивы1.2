using System; 
public class Program {
  static void Main(string[] args) {
    int n = int.Parse(Console.ReadLine());
    double[] array = new double[n];
    double k = 0;
    for (int i = 0;i < n; i++){
      array[i] = double.Parse(Console.ReadLine());
      k += array[i];
    }
    k /= n;
    for (int i = 0;i < n; i++){
      if (array[i] == 0){
        array[i] = k;
      }
    }
    for (int i = 0;i < n; i++){
      Console.WriteLine(array[i]);
    }
  }
} 
