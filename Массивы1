using System; 
public class Program {
  static void Main(string[] args) {
    int n = int.Parse(Console.ReadLine());
    int[] array = new int[n];
    int k = 0;
    for (int i = 0;i < n; i++){
      array[i] = int.Parse(Console.ReadLine());
      if (array[i] > k){
        k = array[i];
      }
    }
    Console.WriteLine(k);
    for (int i = n - 1;i >= 0; i--){
      Console.WriteLine(array[i]);
    }
  }
} 
